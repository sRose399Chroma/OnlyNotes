{
  "manifest_version": 2,

  "name": "Test Extension",
  "description": "My first Google Chrome Extension.",
  "version": "1.0",

  "permissions": [
  	"contextMenus",
  	"tabs",
	"webRequest",
    "http://www.tumblr.com/*"
    "https://www.tumblr.com/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}